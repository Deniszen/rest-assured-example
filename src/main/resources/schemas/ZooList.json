{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "count",
    "animals"
  ],
  "properties": {
    "count": {
      "type": "integer"
    },
    "animals": {
      "type": "array",
      "additionalItems": true,
      "items": {
        "type": "object",
        "additionalProperties": true,
        "required": [
          "id",
          "type",
          "name",
          "age",
          "time"
        ],
        "properties": {
          "id": {
            "type": "integer"
          },
          "type": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "age": {
            "type": "string"
          },
          "time": {
            "type": "string"
          }
        }
      }
    }
  }
}